using System;

namespace Memory_game
{
    public class Spelare : ISpelare
    {
        public string Name { get; init; }
        public Statestik Statestik { get; set; }
        public Spelare(string name, Statestik statestik)
        {
            this.Name = name;
            this.Statestik = statestik;
        }
    }

}
